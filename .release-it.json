{
  "git": {
    "commitMessage": "${version}",
    "tagName": "v${version}"
  },
  "npm": {
    "publish": false,
    "publishPath": ".",
    "tag": "latest",
    "private": false,
    "access": null,
    "otp": null
  },
  "github": {
    "release": true,
    "releaseName": "v${version}",
    "preRelease": false,
    "draft": false,
    "tokenRef": "GITHUB_TOKEN",
    "assets": ["lib/*.tgz", "lib/*.json", "lib/*.bin", "lib/*.wasm", "demo/*.tgz"],
    "host": null,
    "timeout": 0,
    "proxy": false
  }
}
